{
  "name": "ui5-testcafe-selector",
  "version": "0.0.20",
  "description": "Testcafe Selector for UI5",
  "repository": "https://github.com/timostark/ui5TestcafeSelector",
  "main": "lib/index",
  "files": [
    "lib",
    "ts-defs"
  ],
  "scripts": {
    "lint": "eslint src/*.js",
    "http-server": "http-server ./ -s",
