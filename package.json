    "eslint-plugin-testcafe": "^0.2.1",
    "http-server": "^0.11.0",
    "publish-please": "^2.2.0",
    "pug": "^2.0.0-rc.4",
    "testcafe": "*"
  },
  "peerDependencies": {
    "testcafe": "*"
  },
  "keywords": [
    "ui5",
    "openui5",
    "sapui5",
